<div class="pd-top-2">
    <div class="col-md-12 pd-top-2 pd-bottom-2  panel panel-default">
        <form class="form-inline form-search" role="form">
            <div class="form-group col-md-12 col-md-12">
                <label>年级</label>
                <select class="form-control" ng-model="vm.params.grade">
                    <option value="1">一年级</option>
                    <option value="2">二年级</option>
                    <option value="3">三年级</option>
                    <option value="4">四年级</option>
                    <option value="5">五年级</option>
                    <option value="6">六年级</option>
                </select>
                <button class="btn btn-primary pull-right" ng-click="vm.search()">查询</button>
            </div>
        </form>
    </div>

    <div class="row">
        <div class=" col-md-12 ">
            <div class="  panel panel-default">
                <div class="panel-heading">
                    安排课程
                </div>
                <div class="panel-body ">
                    <div class="col-md-10 col-md-offset-1">
                        <datepicker ng-model="vm.data" grade="vm.params.grade"></datepicker>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>